Host nas
    IdentityFile ~/.private/nas.rsa
    User icelk
# This is first, so it'll default to `server`, which has the same domain but occurs later.
Host 10.0.0.16 icelk.dev
    IdentityFile ~/.private/home.rsa
    User icelk
    Port 477
Host server icelk.dev
    IdentityFile ~/.private/server.rsa
    User icelk
Host pi
    IdentityFile ~/.private/pi.rsa
    User pi
Host aur.archlinux.org
    IdentityFile ~/.private/aur.rsa
    User aur
Host server icelk.dev
    IdentityFile ~/.private/server-root.rsa
    User root
