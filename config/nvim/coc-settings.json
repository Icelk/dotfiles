{
    "suggest.noselect": false,
    "coc.preferences.formatOnType": true,
    "coc.preferences.formatOnTypeFiletypes": ["javascript", "typescript", "json", "graphql", "html", "css"],

    "semanticTokens.filetypes": ["*"],

    "rust-analyzer.checkOnSave.command": "clippy",
    "rust-analyzer.cargo.features": "all",
    "rust-analyzer.imports.prefix": "self",
    "rust-analyzer.imports.granularity.group": "module",
    "rust-analyzer.imports.granularity.enforce": true,
    "rust-analyzer.inlayHints.typeHintsSeparator": "",
    "rust-analyzer.inlayHints.chainingHintsSeparator": "",

    "codeLens.enable": true,
    "codeLens.position": "right_align",

    "html.autoClosingTags": true,

    "prettier.semi": false,
    "prettier.tabWidth": 4,
    "prettier.endOfLine": "lf",
    "prettier.printWidth": 120,
    "prettier.trailingComma": "es5"
}
